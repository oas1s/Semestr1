<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>SingUpServlet</servlet-name>
        <servlet-class>servlets.SingUpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SingUpServlet</servlet-name>
        <url-pattern>/signUp</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SignInServlet</servlet-name>
        <servlet-class>servlets.SignInServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignInServlet</servlet-name>
        <url-pattern>/signIn</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>servlets.ProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProfileSettingsServlet</servlet-name>
        <servlet-class>servlets.ProfileSettingsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProfileSettingsServlet</servlet-name>
        <url-pattern>/settings</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetEmailAjaxServlet</servlet-name>
        <servlet-class>servlets.GetEmailAjaxServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    <servlet-name>GetEmailAjaxServlet</servlet-name>
    <url-pattern>/getuseremail</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateProjectServlet</servlet-name>
        <servlet-class>servlets.CreateProjectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateProjectServlet</servlet-name>
        <url-pattern>/createProject</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Project</servlet-name>
        <servlet-class>servlets.ProjectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Project</servlet-name>
        <url-pattern>/project</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>mainPage</servlet-name>
        <servlet-class>servlets.MainPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mainPage</servlet-name>
        <url-pattern>/mainPage</url-pattern>
    </servlet-mapping>
</web-app>